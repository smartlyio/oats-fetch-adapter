{
  "name": "@smartlyio/oats-fetch-adapter",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Fetch adapter for Oats a Openapi3 based generator for typescript aware servers and clients",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rm -rf ./dist && rm -f ./tmp/*.ts",
    "build": "yarn clean && yarn lint && yarn tsc",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^2.13.1",
    "node-fetch": "^2.6.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "form-data": "^4.0.0"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-runtime": "2.18.0",
    "@types/form-data": "2.5.0",
    "@types/node": "14.17.14",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "node-fetch": "3.0.0",
    "prettier": "2.3.2",
    "typescript": "4.4.2"
  }
}
